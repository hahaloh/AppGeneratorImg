<Window x:Class="AppGeneratorImg.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppGeneratorImg"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" >
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/glbg.jpg"/>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <local:butt x:Name="menbt" Content="MEN" HorizontalAlignment="Left" Height="82" Margin="205,229,0,0" VerticalAlignment="Top" Width="146" Click="butt_Clickm" Foreground="#FF006CFF" BorderBrush="{x:Null}"/>
        <local:butt x:Name="girlbt" Content="GIRL" HorizontalAlignment="Left" Height="82" Margin="455,229,0,0" VerticalAlignment="Top" Width="145" Click="butt_ClickG" Foreground="Red" BorderBrush="{x:Null}"/>
    </Grid>
</Window>
